{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "diagnostics.globals": [
    "vim",
    "jit",
    "it",
    "describe",
    "before_each",
    "after_each",
    "setup",
    "teardown",
    "s",
    "sn",
    "isn",
    "t",
    "i",
    "f",
    "c",
    "d",
    "r",
    "events",
    "ai",
    "extras",
    "l",
    "rep",
    "p",
    "m",
    "n",
    "dl",
    "fmt",
    "fmta",
    "conds",
    "postfix",
    "types",
    "parse",
    "ms",
    "k"
  ],
  "diagnostics.disable": [
    "missing-fields"
  ],
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "${3rd}/luv/library"
    ],
    "ignoreDir": [
      "lua/rockyz/snippets"
    ],
    "checkThirdParty": "Disable"
  }
}
